<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.private_void.app.progress_dialog.ProgressDialogController"
            prefHeight="600.0" prefWidth="600.0">

    <VBox prefHeight="600.0" prefWidth="600.0">

        <ProgressBar id="progress_bar" fx:id="progressBar" minWidth="600.0" prefWidth="600.0" />

        <Label id="progress_events" fx:id="progressLabel" minWidth="600.0" prefWidth="600.0" />

    </VBox>

</AnchorPane>
