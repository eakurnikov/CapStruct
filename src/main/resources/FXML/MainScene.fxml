<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.ScatterChart?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<HBox
        xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1"
        maxHeight="-Infinity"
        maxWidth="-Infinity"
        minHeight="-Infinity"
        minWidth="-Infinity"
        prefHeight="700.0"
        prefWidth="820.0" >

    <children>
        <VBox
                prefHeight="700.0"
                prefWidth="300.0">
            <children>
                <Text id="paramarers_text"
                        strokeType="OUTSIDE"
                        strokeWidth="0.0"
                        text="Parameters"
                        textOrigin="CENTER"
                        translateX="110.0">

                    <VBox.margin>
                        <Insets
                                bottom="10.0"
                                left="10.0"
                                right="10.0"
                                top="10.0" />
                    </VBox.margin>
                </Text>

                <TabPane
                        prefHeight="200.0"
                        prefWidth="200.0"
                        tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                        <Tab id="parallelFlux_tab"
                                closable="false"
                                text="Parallel flux">
                            <content>
                                <AnchorPane
                                        minHeight="0.0"
                                        minWidth="0.0"
                                        prefHeight="126.0"
                                        prefWidth="238.0" />
                            </content>
                        </Tab>

                        <Tab id="divergentFlux_tab"
                                closable="false"
                                text="Divergent flux">
                            <content>
                                <AnchorPane
                                        minHeight="0.0"
                                        minWidth="0.0"
                                        prefHeight="180.0"
                                        prefWidth="200.0" />
                            </content>
                        </Tab>
                    </tabs>

                    <VBox.margin>
                        <Insets
                                bottom="10.0"
                                left="10.0"
                                right="10.0"
                                top="10.0" />
                    </VBox.margin>
                </TabPane>

                <TabPane
                        prefHeight="200.0"
                        prefWidth="200.0"
                        tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                        <Tab id="cylinder_tab"
                             closable="false"
                             text="Cylinder">
                            <content>
                                <AnchorPane
                                        minHeight="0.0"
                                        minWidth="0.0"
                                        prefHeight="180.0"
                                        prefWidth="200.0" />
                            </content>
                        </Tab>

                        <Tab id="torus_tab"
                             closable="false"
                             text="Torus">
                            <content>
                                <AnchorPane
                                        minHeight="0.0"
                                        minWidth="0.0"
                                        prefHeight="180.0"
                                        prefWidth="200.0" />
                            </content>
                        </Tab>

                        <Tab id="cone_tab"
                             closable="false"
                             text="Cone">

                            <content>
                                <AnchorPane
                                        minHeight="0.0"
                                        minWidth="0.0"
                                        prefHeight="180.0"
                                        prefWidth="200.0" />
                            </content>
                        </Tab>
                    </tabs>

                    <VBox.margin>
                        <Insets
                                bottom="10.0"
                                left="10.0"
                                right="10.0"
                                top="10.0" />
                    </VBox.margin>
                </TabPane>

            </children>
      </VBox>

       <ScatterChart id="chart" prefHeight="623.0" prefWidth="458.0" title="Scatter chart">
           <xAxis>
               <CategoryAxis side="BOTTOM" />
           </xAxis>
           <yAxis>
               <NumberAxis side="LEFT" />
           </yAxis>
       </ScatterChart>
   </children>
</HBox>
